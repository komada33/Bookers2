<%= render 'books/list', books: @book %>
<table>


  <tbody>
    <tr>
      <td><%= @user.name %></td>
      <td><%= @book.title %></td>
      <td><%= @book.body %></td>
      <td><%= link_to "Edit", edit_book_path(@book.id) %></td>
      <td><%= link_to "destroy", books_path(@book.id), method: :delete, "data-confirm" => "本当に削除しますか？" %></td>
    </tr>
  </tbody>
</table>
