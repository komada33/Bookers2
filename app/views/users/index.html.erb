<%= render 'books/list', books: @book %>
<h1>Users</h1>

<table>
  <thead>
    <tr>
      <td>image</td>
      <td>name</td>
      <td></td>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag @user.get_profile_image(100,100) %></td>
      <td><%= @user.name %></td>
      <td><%= link_to "Show", user_path(:id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>